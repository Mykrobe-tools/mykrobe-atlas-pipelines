              "pipeline": "build",
              "stage": "build.test.integration",
              "job": "build.test.integration",
              "artifact_origin": "external",
              "run_if": "passed",
              "type": "fetch"
            },
            {
              "command": "bash",
              "timeout": -1.0,
              "arguments": [
                "-c",
                "docker run -e DEMO_DATA_ROOT_FOLDER=$DEMO_DATA_ROOT_FOLDER -e LOCATIONIQ_API_KEY=$LOCATIONIQ_API_KEY --rm $DOCKERHUB_USERNAME/atlas-api-test-integration:$GO_DEPENDENCY_LABEL_UPSTREAM yarn test:integration"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "bash",
              "timeout": -1.0,
              "arguments": [
                "-c",
                "curl -X POST -H 'Content-type: application/json' --data '{\"text\":\":ok_hand: API integration tests complete\"}' https://hooks.slack.com/services/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"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "bash",
              "timeout": -1.0,
              "arguments": [
                "-c",
                "curl -X POST -H 'Content-type: application/json' --data '{\"text\":\":disappointed: API integration tests failed\"}' https://hooks.slack.com/services/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"
              ],
              "run_if": "failed",
              "type": "exec"
            }
          ]
        }
      ]
    }
  ]

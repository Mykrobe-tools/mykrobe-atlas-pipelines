  "stages": [
    {
      "name": "test.integration",
      "fetch_materials": true,
      "never_cleanup_artifacts": false,
      "clean_working_directory": false,
      "approval": {
        "type": "success",
        "users": [],
        "roles": []
      },
      "environment_variables": [],
      "jobs": [
        {
          "name": "test.integration",
          "environment_variables": [],
          "tabs": [

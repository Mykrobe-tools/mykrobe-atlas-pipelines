    },
    {
      "name": "LOCATIONIQ_API_KEY",
      "value": "d1c26595029974"
    },
    {
      "name": "DEMO_DATA_ROOT_FOLDER",
      "value": "test/fixtures"
    }
  ],
  "parameters": [],
  "materials": [
    {
      "pipeline": "build",
      "stage": "build.test.integration",
      "name": "upstream",
